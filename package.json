{
  "name": "discord-market-bot",
  "version": "1.0.0",
  "description": "Discord бот для ролевого рынка с лотами, аукционами и сделками",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "setup": "node scripts/setup.js",
    "deploy-commands": "node scripts/deploy-commands.js",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset",
    "db:studio": "prisma studio",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/ tests/",
    "lint:fix": "eslint src/ tests/ --fix"
  },
  "keywords": ["discord", "bot", "market", "economy", "rpg"],
  "author": "Discord Market Bot",
  "license": "MIT",
  "dependencies": {
    "discord.js": "^14.14.1",
    "@prisma/client": "^5.7.1",
    "prisma": "^5.7.1",
    "better-sqlite3": "^9.2.2",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "@jest/globals": "^29.7.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
